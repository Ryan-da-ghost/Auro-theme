{
  "manifest_version": 2,
  "name": "Nord Theme By Ryan The Ghost",
  "version": "1.2",
  "description": "This is the Nord Theme Chrome extension by Ryan the Ghost",
  "author": "Ryan The Ghost",
  "icons": {
    "128": "logo.png"
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "Nord Theme",
    "default_popup": "popup.html"
  },
  "permissions": [],
  "background": {
    "scripts": ["main.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "*://discord.com/*",
        "*://www.youtube.com/*",
        "*://www.deviantart.com/*",
        "*://monkeytype.com/*",
        "*://www.reddit.com/*",
        "*://www.google.com/*"
        ],
      "js": ["javascript/urlCheck.js"]
    }
  ],
  "web_accessible_resources": ["css/*"]
}
